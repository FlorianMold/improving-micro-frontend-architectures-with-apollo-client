  },
  "engines": {
    "node": ">=14.0.0 <17.0.0",
    "npm": ">=8",
    "yarn": "Please-use-npm"
  },
  "scripts": {
    "start": "env-cmd -f .env nx serve",
    "start:host": "env-cmd -f .env nx serve host",
    "start:contact": "env-cmd -f .env nx serve contact",
    "start:sales": "env-cmd -f .env nx serve sales",
    "start:dashboard": "env-cmd -f .env nx serve dashboard",
    "start:user": "env-cmd -f .env nx serve user",
    "start:dashboard-react": "env-cmd -f .env nx serve dashboard-react",
    "build": "env-cmd -f .env nx build",
    "build:host": "env-cmd -f .env nx build host",
    "build:contact": "env-cmd -f .env nx build contact",
    "build:sales": "env-cmd -f .env nx build sales",
    "build:user": "env-cmd -f .env nx build user",
    "build:dashboard": "env-cmd -f .env nx build dashboard",
    "build:dashboard-react": "env-cmd -f .env nx build dashboard-react",
    "build:ci:production": "nx run-many --target=build --all",
    "build:ci:development": "nx run-many --target=build --all --configuration=development",
    "test": "nx test",
    "postinstall": "ngcc --properties es2020 browser module main",
    "prepare": "husky install",
    "prettier:check": "prettier --config .prettierrc --check \"src/**/*.{ts,css,html}\"",
    "pre-commit": "lint-staged",
    "dockerize": "docker-compose up --build",
    "sonar": "sonar-scanner",
    "sync-settings": "env-cmd -f .env node scripts/sync-settings.js",
    "set-module-federation:host": "env-cmd -f .env node scripts/set-module-federation.js",
    "generate:contact:mock-data": "ts-node scripts/mocks/contact/generate-contact-mock-data.ts 10 libs/contact/data-access/src/lib/mock-data",
    "merge:contact:mock-data": "ts-node scripts/mocks/contact/merge-and-store-data.ts mock-data/contact-mock-data.json",
    "generate:sales:mock-data": "ts-node scripts/mocks/sales/generate-sales-mock-data.ts 10 libs/sales/data-access/src/lib/mock-data",
    "merge:sales:mock-data": "ts-node scripts/mocks/sales/merge-and-store-data.ts mock-data/sales-mock-data.json",
    "generate:shared:mock-data": "ts-node scripts/mocks/shared/generate-shared-mock-data.ts 2 libs/shared/data-access/src/lib/mock-data",
    "merge:shared:mock-data": "ts-node scripts/mocks/shared/merge-and-store-data.ts mock-data/shared-mock-data.json"
  },
  "author": {
    "name": "Florian Mold",
    "email": "florian.mold@live.at"
  },
  "private": true,
