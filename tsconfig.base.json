{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "module": "esnext",
    "lib": ["es2017", "dom", "esnext.asynciterable"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "noUnusedParameters": true,
    "noFallthroughCasesInSwitch": true,
    "noUnusedLocals": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "noImplicitOverride": true,
    "noImplicitReturns": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "useUnknownInCatchVariables": true,
    "forceConsistentCasingInFileNames": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@ui-frontend-service/contact/api-types": ["libs/contact/api-types/src/index.ts"],
      "@ui-frontend-service/contact/data-access": ["libs/contact/data-access/src/index.ts"],
      "@ui-frontend-service/host/data-access": ["libs/host/data-access/src/index.ts"],
      "@ui-frontend-service/sales/api-types": ["libs/sales/api-types/src/index.ts"],
      "@ui-frontend-service/sales/data-access": ["libs/sales/data-access/src/index.ts"],
      "@ui-frontend-service/shared/api-types": ["libs/shared/api-types/src/index.ts"],
      "@ui-frontend-service/shared/data-access": ["libs/shared/data-access/src/index.ts"],
      "@ui-frontend-service/shared/feature/common-date-options": ["libs/shared/feature/common-date-options/src/index.ts"],
      "@ui-frontend-service/shared/feature/common-options": ["libs/shared/feature/common-options/src/index.ts"],
      "@ui-frontend-service/shared/feature/common-services": ["libs/shared/feature/common-services/src/index.ts"],
      "@ui-frontend-service/shared/feature/graphql-augmented-operations": [
        "libs/shared/feature/graphql-augmented-operations/src/index.ts"
      ],
      "@ui-frontend-service/shared/feature/graphql-client-options": ["libs/shared/feature/graphql-client-options/src/index.ts"],
      "@ui-frontend-service/shared/feature/logger": ["libs/shared/feature/logger/src/index.ts"],
      "@ui-frontend-service/shared/types/graphql-client-types": ["libs/shared/types/graphql-client-types/src/index.ts"],
      "@ui-frontend-service/shared/ui/common-app-styles": ["libs/shared/ui/common-app-styles/src/index.ts"],
      "@ui-frontend-service/shared/ui/components/address": ["libs/shared/ui/components/address/src/index.ts"],
      "@ui-frontend-service/shared/ui/components/customer": ["libs/shared/ui/components/customer/src/index.ts"],
      "@ui-frontend-service/shared/ui/components/delete-dialog": ["libs/shared/ui/components/delete-dialog/src/index.ts"],
      "@ui-frontend-service/shared/ui/components/form-submit": ["libs/shared/ui/components/form-submit/src/index.ts"],
      "@ui-frontend-service/shared/ui/components/login": ["libs/shared/ui/components/login/src/index.ts"],
      "@ui-frontend-service/shared/ui/components/profile": ["libs/shared/ui/components/profile/src/index.ts"],
      "@ui-frontend-service/shared/ui/components/user": ["libs/shared/ui/components/user/src/index.ts"],
      "@ui-frontend-service/shared/ui/components/version": ["libs/shared/ui/components/version/src/index.ts"],
      "@ui-frontend-service/shared/ui/dom-layout": ["libs/shared/ui/dom-layout/src/index.ts"],
      "@ui-frontend-service/shared/ui/grid": ["libs/shared/ui/grid/src/index.ts"],
      "@ui-frontend-service/shared/ui/ng-react": ["libs/shared/ui/ng-react/src/index.ts"],
      "@ui-frontend-service/shared/ui/resizable": ["libs/shared/ui/resizable/src/index.ts"],
      "@ui-frontend-service/shared/ui/toastr": ["libs/shared/ui/toastr/src/index.ts"],
      "@ui-frontend-service/shared/util/functions": ["libs/shared/util/functions/src/index.ts"],
      "@ui-frontend-service/shared/util/types": ["libs/shared/util/types/src/index.ts"]
    }
  },
  "exclude": ["node_modules", "tmp"]
}
